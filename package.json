{
  "name": "necron-discord-bot",
  "version": "0.1.0",
  "private": true,
  "description": "Necron discord bot",
  "repository": "https://github.com/Necron-Network/necron-discord-bot",
  "license": "AGPL-3.0",
  "author": "Mednoob",
  "scripts": {
    "build": "npm run lint && npm run compile",
